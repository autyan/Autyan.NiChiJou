{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    }
  },
  "Session": {
    "Expiration": 120
  },
  "DistributedCache": {
    "Server": "localhost",
    "Instance": "Autyan.Distributed"
  },
  "IdentityCache": {
    "Server": "localhost",
    "Instance": "Autyan.Identity"
  },
  "ConnectionStrings": {
    "Default": "Data Source=localhost;Initial Catalog=Autyan.NiChiJou.Identity;Integrated Security=True"
  },
  "Cookie": {
    "LoginPath": "/Auth/Login",
    "LogoutPath": "/Auth/Logout",
    "RegisterPath": "/Auth/Register",
    "Schema": "autyan.cookie",
    "Expiration": 30
  },
  "ServiceToken": {
    "Schema": "autyan.serviceToken",
    "MaxAge": 300
  },
  "UnifyLogin": {
    "VerifyTokenAddress": "",
    "MemberAccessAddress": "",
    "AppId": "",
    "ApiKey": "" 
  } 
}
